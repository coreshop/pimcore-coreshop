{
  "name": "coreshop/core",
  "type": "library",
  "description": "CoreShop Core Component (Glue)",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "CoreShop Team",
      "email": "info@coreshop.com",
      "homepage": "https://www.coreshop.com/",
      "role": "Developer"
    },
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "coreshop/configuration": "^4.0",
    "coreshop/store": "^4.0",
    "coreshop/resource": "^4.0",
    "coreshop/address": "^4.0",
    "coreshop/order": "^4.0",
    "coreshop/currency": "^4.0",
    "coreshop/customer": "^4.0",
    "coreshop/shipping": "^4.0",
    "coreshop/inventory": "^4.0",
    "coreshop/product": "^4.0",
    "coreshop/product-quantity-price-rules": "^4.0",
    "coreshop/index": "^4.0",
    "coreshop/payment": "^4.0",
    "coreshop/payum-payment": "^4.0",
    "coreshop/taxation": "^4.0",
    "coreshop/notification": "^4.0",
    "coreshop/payum-bundle": "^4.0",
    "coreshop/variant": "^4.0",
    "coreshop/seo": "^4.0",
    "coreshop/user": "^4.0",
    "coreshop/tracking": "^4.0",
    "coreshop/wishlist": "^4.0",
    "pimcore/pimcore": "^11.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "phpstan/phpstan-webmozart-assert": "^1.2.4"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Component\\Core\\": ""
    }
  },
  "repositories": [
      {
          "type": "path",
          "url": "../../*/*"
      }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
   "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
