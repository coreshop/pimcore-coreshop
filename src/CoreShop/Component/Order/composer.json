{
  "name": "coreshop/order",
  "type": "library",
  "description": "CoreShop Order Component.",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "CoreShop Team",
      "email": "info@coreshop.com",
      "homepage": "https://www.coreshop.com/",
      "role": "Developer"
    },
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "coreshop/address": "^3.0",
    "coreshop/rule": "^3.0",
    "coreshop/registry": "^3.0",
    "coreshop/resource": "^3.0",
    "coreshop/sequence": "^3.0",
    "coreshop/store": "^3.0",
    "coreshop/currency": "^3.0",
    "coreshop/taxation": "^3.0",
    "coreshop/payment": "^3.0",
    "coreshop/pimcore": "^3.0",
    "coreshop/customer": "^3.0",
    "coreshop/product": "^3.0",
    "coreshop/product-quantity-price-rules": "^3.0",
    "coreshop/storage-list": "^3.0",
    "coreshop/workflow-bundle": "^3.0",
    "pimcore/pimcore": "^10.6",
    "laminas/laminas-stdlib": "^3.6"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "phpstan/phpstan-webmozart-assert": "^1.2.4"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Component\\Order\\": ""
    }
  },
  "repositories": [
      {
          "type": "path",
          "url": "../../*/*"
      }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-next": "3.1-dev",
      "dev-master": "3.1-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
