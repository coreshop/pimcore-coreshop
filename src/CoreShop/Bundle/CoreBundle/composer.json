{
  "name": "coreshop/core-bundle",
  "type": "pimcore-bundle",
  "description": "CoreShop - Core Bundle (Glue Bundle)",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "CoreShop Team",
      "email": "info@coreshop.com",
      "homepage": "https://www.coreshop.com/",
      "role": "Developer"
    },
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "coreshop/order-bundle": "^4.0",
    "coreshop/customer-bundle": "^4.0",
    "coreshop/inventory-bundle": "^4.0",
    "coreshop/product-bundle": "^4.0",
    "coreshop/product-quantity-price-rules-bundle": "^4.0",
    "coreshop/address-bundle": "^4.0",
    "coreshop/currency-bundle": "^4.0",
    "coreshop/money-bundle": "^4.0",
    "coreshop/taxation-bundle": "^4.0",
    "coreshop/store-bundle": "^4.0",
    "coreshop/index-bundle": "^4.0",
    "coreshop/payment-bundle": "^4.0",
    "coreshop/seo-bundle": "^4.0",
    "coreshop/shipping-bundle": "^4.0",
    "coreshop/tracking-bundle": "^4.0",
    "coreshop/frontend-bundle": "^4.0",
    "coreshop/sequence-bundle": "^4.0",
    "coreshop/payum-bundle": "^4.0",
    "coreshop/payum-payment-bundle": "^4.0",
    "coreshop/configuration-bundle": "^4.0",
    "coreshop/notification-bundle": "^4.0",
    "coreshop/workflow-bundle": "^4.0",
    "coreshop/menu-bundle": "^4.0",
    "coreshop/user-bundle": "^4.0",
    "coreshop/variant-bundle": "^4.0",
    "coreshop/wishlist-bundle": "^4.0",
    "coreshop/messenger-bundle": "^4.0",
    "coreshop/class-definition-patch-bundle": "^4.0",
    "coreshop/core": "^4.0",
    "doctrine/data-fixtures": "^1.5",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "fakerphp/faker": "^1.16",
    "rinvex/countries": "^7.3",
    "pimcore/pimcore": "^11.0 || ^12.0",
    "pimcore/newsletter-bundle": "^1.0.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "phpstan/phpstan-webmozart-assert": "^1.2.4"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\CoreBundle\\": ""
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../*/*"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.1-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
